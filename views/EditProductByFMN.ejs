<!-- 新增產品表單 -->
<div class="wrap" style="width:50%;height:100%;margin:0 auto;margin-top:60px;margin-bottom:60px;border:1px solid #dbdbdb;border-radius: 25px;padding:20px;">
    <form name="editform" id="editform" action="/Product/POST/EditProductByFMN" method="post" accept-charset="utf-8">
        <div class="form-row">
        <div class="form-group col-md-4">
            <label for="p_SYMBOL">產品代號</label>
            <input type="text" class="form-control" name="p_SYMBOL" id="p_SYMBOL" placeholder="ex:NCCU19" value="<%=data[0].p_SYMBOL %>" readonly>
        </div>
        <div class="form-group col-md-4">
                <label for="p_name">產品名稱</label>
                <input type="text" class="form-control" name="p_name" id="p_name" placeholder="ex:政大一號" value="<%=data[0].p_name %>">
        </div>
        <div class="form-group col-md-4">
                <label for="p_location">產品所在地</label>
                <input type="text" class="form-control" name="p_location" id="p_location" placeholder="ex:台北" value="<%=data[0].p_location %>">
        </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                <label for="p_pricing">產品定價</label>
                <input type="text" class="form-control" name="p_pricing" id="p_pricing" placeholder="ex:500" value="<%=data[0].p_pricing %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_duration">專案年期</label>
                    <input type="text" class="form-control" name="p_duration" id="p_duration" placeholder="ex:10" value="<%=data[0].p_duration %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_currency">幣別</label>
                    <input type="text" class="form-control" name="p_currency" id="p_currency" placeholder="ex:NTD" value="<%=data[0].p_currency %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                <label for="p_irr">投報率</label>
                <input type="text" class="form-control" name="p_irr" id="p_irr" placeholder="ex:4.8" value="<%=data[0].p_irr %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_releasedate">Token發行日</label>
                    <input type="text" class="form-control" name="p_releasedate" id="p_releasedate" placeholder="ex:12312019" value="<%=data[0].p_releasedate %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_validdate">Token到期日</label>
                    <input type="text" class="form-control" name="p_validdate" id="p_validdate" placeholder="ex:12312022" value="<%=data[0].p_validdate %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-6">
                <label for="p_size">案場規模(kw)</label>
                <input type="text" class="form-control" name="p_size" id="p_size" placeholder="ex:8" value="<%=data[0].p_size %>">
                </div>
                <div class="form-group col-md-6">
                    <label for="p_totalrelease">總發行量(token)</label>
                    <input type="text" class="form-control" name="p_totalrelease" id="p_totalrelease" placeholder="ex:999" value="<%=data[0].p_totalrelease %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                <label for="p_FAY">預估平均年收益</label>
                <input type="text" class="form-control" name="p_FAY" id="p_FAY" placeholder="ex:4.8" value="<%=data[0].p_FAY %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_FTRT">預估回收金額</label>
                    <input type="text" class="form-control" name="p_FTRT" id="p_FTRT" placeholder="ex:15,327" value="<%=data[0].p_FTRT %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_RPT">租金支付週期每幾個月</label>
                    <input type="text" class="form-control" name="p_RPT" id="p_RPT" placeholder="ex:" value="<%=data[0].p_RPT %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-6">
                <label for="p_FRP">預估回收年限</label>
                <input type="text" class="form-control" name="p_FRP" id="p_FRP" placeholder="ex:8" value="<%=data[0].p_FRP %>">
                </div>
                <div class="form-group col-md-6">
                    <label for="p_Timeline">預估進度圖</label>
                    <input type="text" class="form-control" name="p_Timeline" id="p_Timeline" placeholder="ex:" value="<%=data[0].p_Timeline %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                    <label for="p_PSD">專案開始日期</label>
                    <input type="text" class="form-control" name="p_PSD" id="p_PSD" placeholder="ex:20191231" value="<%=data[0].p_PSD %>">
                </div>
                <div class="form-group col-md-4">
                  <label for="p_CFSD">開始購買/眾籌日期</label>
                  <input type="text" class="form-control" name="p_CFSD" id="p_CFSD" placeholder="ex:20191231" value="<%=data[0].p_CFSD %>">
                </div>
                <div class="form-group col-md-4">
                      <label for="p_CFED">結束購買/眾籌日期</label>
                      <input type="text" class="form-control" name="p_CFED" id="p_CFED" placeholder="ex:20200331" value="<%=data[0].p_CFED %>">
                 </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-6">
                  <label for="p_TaiPowerApprovalDate">台電並聯作業日期</label>
                  <input type="text" class="form-control" name="p_TaiPowerApprovalDate" id="p_TaiPowerApprovalDate" placeholder="ex:20191231" value="<%=data[0].p_TaiPowerApprovalDate %>">
                </div>
                <div class="form-group col-md-6">
                      <label for="p_BOEApprovalDate">能源局同意備案日期</label>
                      <input type="text" class="form-control" name="p_BOEApprovalDate" id="p_BOEApprovalDate" placeholder="ex:20200331" value="<%=data[0].p_BOEApprovalDate %>">
