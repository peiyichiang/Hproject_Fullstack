<div class="wrap">
        <div class="clear"></div>
        <div class="content">
            <div class="signup">
                <div class="up">
                    <!-- <img style="width: 90px;" src="data:image/png;base64," alt=""> -->
                    <!-- <div class="textInfo">享受美食好味道</div> -->
                </div>

                <div class="down">
                    <div class="signUpWindow">
                        <div class="windowUp">
                            <div class="title">
                                編輯產品資料
                            </div>
                        </div>
                        <div class="windowDown">
                            <div class="right">
                                <div class="login">
                                    <form name="editform" id="editform" action="/Product/POST/EditProductByFMN" method="post" accept-charset="utf-8">
                                                <div class="inputTitle">Token代號</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_SYMBOL" placeholder="p_SYMBOL" value="<%=data[0].p_SYMBOL %>" readonly/>
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">產品名稱</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_name" placeholder="p_name" value="<%=data[0].p_name %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">產品所在地</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_location" placeholder="p_location" value="<%=data[0].p_location %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">單片售價</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_pricing" placeholder="p_pricing" value="<%=data[0].p_pricing %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">專案年期</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_duration" placeholder="p_duration" value="<%=data[0].p_duration %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">貨幣種類</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_currency" placeholder="p_currency" value="<%=data[0].p_currency %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">投報率</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_irr" placeholder="p_irr" value="<%=data[0].p_irr %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">發行日</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_releasedate" placeholder="p_releasedate" value="<%=data[0].p_releasedate %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">到期日</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_validdate" placeholder="p_validdate" value="<%=data[0].p_validdate %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">案場規模</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_size" placeholder="p_size" value="<%=data[0].p_size %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">總發行量</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_totalrelease" placeholder="p_totalrelease" value="<%=data[0].p_totalrelease %>" />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">產品icon</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_icon" id="p_icon" placeholder="p_icon" value="<%=data[0].p_icon %>" readonly />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>

                                                <div class="inputTitle">產品文件</div>
                                                <div class="inputContainer">
                                                        <input type="text" class="username" name="p_assetdocs" id="p_assetdocs" placeholder="p_assetdocs" value="<%=data[0].p_assetdocs %>" readonly />
                                                        <i class="fas fa-id-badge"></i>
                                                </div>
                                                <div class="haveAccount responseState"></div>
                                        </form>
                                        <form action="/upload" id="FileForm" method="post" enctype="multipart/form-data">
                                            <div class="inputTitle">上傳產品文件</div>
                                            <input type="file" name="file" style="margin-bottom:10px;" />
                                            <div class="inputTitle">上傳產品icon</div>
                                            <input type="file" name="icon" />
                                        </form>
                                        <input type='button' value="修改" id="modify" class="modify">
                                    <div class="clear"></div>
                                </div>
                            </div>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="transaction">
                
            </div>
        </div>

        <div class="clear"></div>

        <!-- <div class="footer">
            <div class="Info">
                <div class="title">@NCCU Blockchain Hub</div>

            </div>
        </div> -->
</div>

<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<script src="/js/EditProductByFMN.js"></script>
</body>
</html>