<div class="wrap" style="width:50%;height:100%;margin:0 auto;margin-top:60px;margin-bottom:60px;border:1px solid #dbdbdb;border-radius: 25px;padding:20px;">
    <form name="editform" id="editform" action="/Product/EditProductByFMN" method="post" accept-charset="utf-8">
        <div class="form-row">
        <div class="form-group col-md-4">
            <label for="p_SYMBOL">產品代號</label>
            <input type="text" class="form-control" name="p_SYMBOL" id="p_SYMBOL" placeholder="ex:NCCU19" value="<%=data[0].p_SYMBOL %>" readonly>
        </div>
        <div class="form-group col-md-4">
                <label for="p_name">產品名稱</label>
                <input type="text" class="form-control" name="p_name" id="p_name" placeholder="ex:政大一號" value="<%=data[0].p_name %>">
        </div>
        <div class="form-group col-md-4">
                <label for="p_location">產品所在地</label>
                <input type="text" class="form-control" name="p_location" id="p_location" placeholder="ex:台北" value="<%=data[0].p_location %>">
        </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                <label for="p_pricing">產品定價</label>
                <input type="text" class="form-control" name="p_pricing" id="p_pricing" placeholder="ex:500" value="<%=data[0].p_pricing %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_duration">專案年期</label>
                    <input type="text" class="form-control" name="p_duration" id="p_duration" placeholder="ex:10" value="<%=data[0].p_duration %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_currency">幣別</label>
                    <input type="text" class="form-control" name="p_currency" id="p_currency" placeholder="ex:NTD" value="<%=data[0].p_currency %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                <label for="p_irr">預期報酬率(%)</label>
                <input type="text" class="form-control" name="p_irr" id="p_irr" placeholder="ex:4.8" value="<%=data[0].p_irr %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_releasedate">產品發行日</label>
                    <input type="text" class="form-control" name="p_releasedate" id="p_releasedate" placeholder="ex:YYYYMMDDHHMM" value="<%=data[0].p_releasedate %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_validdate">產品到期日</label>
                    <input type="text" class="form-control" name="p_validdate" id="p_validdate" placeholder="ex:YYYYMMDDHHMM" value="<%=data[0].p_validdate %>">
                </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                <label for="p_size">專案規模(kw)</label>
                <input type="text" class="form-control" name="p_size" id="p_size" placeholder="ex:8" value="<%=data[0].p_size %>">
                </div>
                <div class="form-group col-md-4">
                    <label for="p_totalrelease">產品總發行量(個)</label>
                    <input type="text" class="form-control" name="p_totalrelease" id="p_totalrelease" placeholder="ex:330" value="<%=data[0].p_totalrelease %>">
                </div>
                <div class="form-group col-md-4">
                  <label for="p_fundingType">募款類型</label>
                    <br>
                    <%if (data[0].p_fundingType =="1") { %>
                      <label>
                        <input type="radio"  name="p_fundingType" id="p_fundingType" value="1" checked>公募(PO)
                      </label>
                      <label>
                        <input type="radio"  name="p_fundingType" id="p_fundingType" value="2">私募(PP)
                      </label>
                    <% } %>
                    <%if (data[0].p_fundingType =="2") { %>
                      <label>
                        <input type="radio"  name="p_fundingType" id="p_fundingType" value="1">公募
                      </label>
                      <label>
                        <input type="radio"  name="p_fundingType" id="p_fundingType" value="2" checked>私募
                      </label>
                    <% } %>
                 </div>
        </div>
        <div class="form-row">
                <div class="form-group col-md-4">
                  <label for="p_isNewCase">舊案/新案</label>
                    <br>
                    <%if (data[0].p_isNewCase =="1") { %>
                      <label>
                        <input type="radio"  name="p_isNewCase" id="p_isNewCase" value="1" checked>舊案
                      </label>
                      <label>
                        <input type="radio"  name="p_isNewCase" id="p_isNewCase" value="2">新案
                      </label>
                    <% } %>
                    <%if (data[0].p_isNewCase =="2") { %>
                      <label>
                        <input type="radio"  name="p_isNewCase" id="p_isNewCase" value="1">舊案
                      </label>
                      <label>
                        <input type="radio"  name="p_isNewCase" id="p_isNewCase" value="2" checked>新案
                      </label>
                    <% } %>
                </div>
                <div class="form-group col-md-4">
                    <label for="p_RPT">收益總期數</label>
                    <input type="text" class="form-control" name="p_RPT" id="p_RPT" placeholder="ex:80" value="<%=data[0].p_RPT %>">
                </div>
                <div class="form-group col-md-4">
                  <label for="p_FRP">預估回收年限(年數/月數)</label>
                  <input type="text" class="form-control" name="p_FRP" id="p_FRP" placeholder="ex:20/10" value="<%=data[0].p_FRP %>">
                  </div>
        </div>
        <div class="form-row">
          <div class="form-group col-md-4">
