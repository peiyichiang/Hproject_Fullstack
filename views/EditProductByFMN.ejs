            <input type="text" class="form-control" name="p_fundingGoal" id="p_fundingGoal" placeholder="ex:800" value="<%=data[0].p_fundingGoal %>">
          </div>
          <div class="form-group col-md-4">
            <label for="p_HCAT721uri">專案於公鏈或政府官方文件</label>
            <input type="text" class="form-control" name="p_HCAT721uri" id="p_HCAT721uri" placeholder="" value="<%=data[0].p_HCAT721uri %>">
          </div>
