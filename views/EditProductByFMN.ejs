                  <input type="text" class="form-control" name="p_PVTrialOperationDate" id="p_PVTrialOperationDate" placeholder="ex:20191231" value="<%=data[0].p_PVTrialOperationDate %>">
                </div>
                <div class="form-group col-md-6">
                      <label for="p_PVOnGridDate">案場建置完成/電費開始計費日期</label>
                      <input type="text" class="form-control" name="p_PVOnGridDate" id="p_PVOnGridDate" placeholder="ex:20200331" value="<%=data[0].p_PVOnGridDate %>">
                 </div>
        </div>
        <div class="form-row" hidden>
                <div class="form-group col-md-6">
                <label for="p_assetdocs">產品文件</label>
                <input type="text" class="form-control" name="p_assetdocs" id="p_assetdocs" value="<%=data[0].p_assetdocs %>" readonly>
                </div>
                <div class="form-group col-md-6">
