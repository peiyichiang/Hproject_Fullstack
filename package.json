{
  "name": "hproject",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "solc": "cd ethereum/contracts && node zcompile.js",
    "deploy": "cd ethereum/contracts && node zdeploy.js",
    "livechain": "cd ethereum/contracts && node zlivechain.js",
    "start": "nodemon ./bin/www",
    "timeserver": "node timeserver/timeserver.js",
    "manager": "node timeserver/manager/manager.js",
    "rent": "node timeserver/manager/rent.js",
    "crowdfunding": "node timeserver/manager/crowdfunding.js",
    "order": "node timeserver/manager/order.js",
    "tokencontroller": "node timeserver/manager/tokencontroller.js",
    "time": "concurrently -n timeserver,manager,rent,crowdfunding,order,tokencontroller \"npm run timeserver\" \"npm run manager\" \"npm run rent\" \"npm run crowdfunding\" \"npm run order\" \"npm run tokencontroller\"",
    "test": "mocha",
    "testcf": "mocha --grep CrowdFunding",
    "testhcat": "mocha --grep HCAT721",
    "testim": "mocha --grep IncomeManagerCtrt",
    "testasset": "mocha --grep AssetBook",
    "testregistry": "mocha --grep Registry",
    "testpmgr": "mocha --grep ProductManager",
    "testarray": "mocha --grep ArrayUtils"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ejs": "~2.5.7",
    "ethereumjs-tx": "^1.3.7",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.4.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "node-schedule": "^1.3.1",
    "nodemailer": "^4.7.0",
    "nodemon": "^1.18.10",
    "request": "^2.88.0",
    "truffle-privatekey-provider": "^1.0.0",
    "typedarray-to-buffer": "^3.1.5",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "async": "^2.6.1",
    "ganache-cli": "^6.3.0",
    "solc": "^0.5.4"
  }
}
